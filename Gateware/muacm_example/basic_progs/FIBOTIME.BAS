5 REM FIBOTIME.BAS
6 REM USES THE US TIMER TO MEASURE RUN SPEED
10 L=0
20 A=0
30 B=1
40 C=0
50 S=(65536*PEEK(243*256+2))+(256*PEEK(243*256+1))+PEEK(243*256)
70 PRINT C
90 C = A+B
100 A=B
110 B=C
120 L=L+1
130 IF L<180 THEN GOTO 70
140 E=(65536*PEEK(243*256+2))+(256*PEEK(243*256+1))+PEEK(243*256)
150 T=(E-S)/1000
160 PRINT T;
170 PRINT "MS"
180 PRINT T/180;
190 PRINT "MS / ITERATION"